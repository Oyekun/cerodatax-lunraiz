{
   "name": "cerodatax",
   "version": "1.0.0.0",
   "indexHtmlPath": "index.html",
   "classpath": "${app.dir}/app,${app.dir}/app/override/form/field/",
   "overrides": "${app.dir}/overrides,${app.dir}/locale/${app.locale}",
   "framework": "ext",
   "theme": "ext-theme-crisp-42cb23ca-ec91-40f2-8b23-e99d45e34d8a",
   "requires": [
      "ext-locale"
   ],
   "locale": "es",
   "sass": {
